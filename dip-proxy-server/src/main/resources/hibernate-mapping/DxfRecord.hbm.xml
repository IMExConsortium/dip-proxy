<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="edu.ucla.mbi.cache.DxfRecord" table="dxf_record" lazy="false">
    <id name="id" column="pk">
	<generator class="sequence">
		<param name="sequence">dxf_record_pk_seq</param>
	</generator>
    </id>

    <property name="provider" column="provider" />
    <property name="service" column="service" />
    <property name="ns"  column="ns"/>
    <property name="ac"  column="ac"/>
    <property name="detail"  column="detail"/>
    <property name="dxf" column="dxf"/>
    <property name="createTime" column="create_time"/>
    <property name="queryTime" column="query_time"/>
    <property name="expireTime" column="expire_time"/>    
  </class>
</hibernate-mapping>


