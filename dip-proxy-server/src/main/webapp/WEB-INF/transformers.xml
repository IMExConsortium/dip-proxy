<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:aop="http://www.springframework.org/schema/aop"
    xmlns:tx="http://www.springframework.org/schema/tx"
    xsi:schemaLocation="
    http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
    http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-2.0.xsd
    http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.0.xsd">

 <bean id="ebiTransformer"
       class="edu.ucla.mbi.proxy.ebi.EbiProxyTransformer">
  <property name="transfMap">
   <map>
    <entry key="uniprot">
     <value>/xsl/ebi/uniprot2dxf.xsl</value>
    </entry>
    <entry key="picr">
     <value>/xsl/ebi/picr2dxf.xsl</value>
    </entry>
   </map>
  </property>
  <!--
  <property name="context">
   <map>
    <entry key="ebiProxy">
      <ref bean="ebiReferenceProxy"/>
     </entry> 
    <entry key="ncbiProxy">
      <ref bean="ncbiReferenceProxy"/>
     </entry> 
   </map> 
  </property>
  -->
 </bean>

 <bean id="ncbiTransformer"
       class="edu.ucla.mbi.proxy.ProxyTransformer">
  <property name="transfMap">
   <map>
    <entry key="nlm">
     <value>/xsl/ncbi/nlm2dxf.xsl</value>
    </entry>
    <entry key="refseq">
     <value>/xsl/ncbi/refseq2dxf.xsl</value>
    </entry>
    <entry key="entrezgene">
     <value>/xsl/ncbi/gene2dxf.xsl</value>
    </entry>
    <entry key="pubmed">
     <value>/xsl/ncbi/pubmed2dxf.xsl</value>
    </entry>
    <entry key="taxon">
     <value>/xsl/ncbi/taxon2dxf.xsl</value>
    </entry>
   </map>
  </property>
 </bean>

 <bean id="prlTransformer"
    class="edu.ucla.mbi.proxy.ProxyTransformer">
  <property name="transfMap">
    <map>
    </map>
  </property>
  <property name="context">
   <map/>
<!--
    <entry key="ncbiProxy">  
     <ref bean="ncbiReferenceServer"/>
    </entry>
   </map>
-->
  </property>
 </bean>

 <bean id="dipTransformer"
    class="edu.ucla.mbi.proxy.ProxyTransformer">
  <property name="transfMap">
    <map>
     <entry key="dip">
      <value>/xsl/pretty-print.xsl</value>
     </entry>
    </map>
  </property>
  <property name="context">
   <map/>
  </property>
 </bean>

</beans>
